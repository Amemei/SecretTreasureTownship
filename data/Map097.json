{"id":2,"name":"EV002","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"tileId":0,"characterName":"Monster","direction":6,"pattern":1,"characterIndex":6},"list":[{"code":243,"indent":0,"parameters":[]},{"code":241,"indent":0,"parameters":[{"name":"ダンジョン m-art_Trial","volume":45,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["カサベラ"]},{"code":401,"indent":0,"parameters":["「おやおや……こんなところから、抜け出そうなんて。"]},{"code":401,"indent":0,"parameters":["　なるほど、貴方が手助けしたのですね、コゼット」"]},{"code":231,"indent":0,"parameters":[2,"クノ_私服_怒り",0,0,30,24,100,100,255,0]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["コゼット"]},{"code":401,"indent":0,"parameters":["「ちっ……よりにもよって、あんたに見つかるとはな。"]},{"code":401,"indent":0,"parameters":["　カサベラッ！！」"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["カサベラ"]},{"code":401,"indent":0,"parameters":["「フォフォフォ……相変わらず、口の聞き方を知らない子だ。"]},{"code":401,"indent":0,"parameters":["　ところで……そちらが、龍の少年か」"]},{"code":231,"indent":0,"parameters":[1,"ユーク_私服_怒り",0,0,480,24,100,100,255,0]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["サキト"]},{"code":401,"indent":0,"parameters":["「俺たちをどうするつもりだ！　お前の目的はなんだっ！！」"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["カサベラ"]},{"code":401,"indent":0,"parameters":["「このような少年が、龍に選ばれるとは……『龍伝説』も、"]},{"code":401,"indent":0,"parameters":["　大したことはありませんね……」"]},{"code":231,"indent":0,"parameters":[2,"レイナ_私服_怒り",0,0,30,24,100,100,255,0]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["オーレリー"]},{"code":401,"indent":0,"parameters":["「何故、知っているのです！　彼のことを！！」"]},{"code":101,"ind